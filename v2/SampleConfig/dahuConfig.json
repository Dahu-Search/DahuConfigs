{
  "logLevel":"TRACE",
  "name":"DEF 4 AODocs",
  "threads":"40",
  "admin":"true",
  "adminUI":"false",
  "searchUI": "true",
  "DEFprotocol": "https",
  "httpPort": "80",
  "httpsPort": "443",
  "uiDocRoot": "./docRoot",
  "plugins":"./lib",
  "crossOrigin":"true",
  "responseVersion":"2",
  "securityTokenTTL":"3600",
  "DEFRolesFile": "roles.json",
  "commands":[
      {
        "name": "Surface",
        "jar": "DahuSurface.jar",
        "class": "com.dahu.surface.commands.Surface",
        "settingsFile": "Surface.json"
      }
  ],
  "proxies":[
  ],
  "services":[
    {
      "name":"AODocsCrawlerMT",
      "jar":"DahuEdge.jar",
      "class":"com.dahu.plugins.edge.aodocs.AODocsMTConnector",
      "settingsFile":"AODocsLibraries.json",
      "threads":"1"
    },
    {
      "name":"Vector",
      "class":"com.dahu.vector.core.VectorService",
      "settingsFile":"vector.json",
      "threads":"1"
    }

  ],
  "stores":[
    {
      "name":"AODocsConnectorStore",
      "jar":"DahuEdge.jar",
      "class":"com.dahu.plugins.edge.aodocs.storage.MongoStore"
    },
    {
      "name": "SurfaceCacheStore",
      "jar": "DahuSurface.jar",
      "class": "com.dahu.surface.stores.SurfaceCacheStore"
    },
    {
      "name": "SurfaceMongoStore",
      "jar": "DahuSurface.jar",
      "class": "com.dahu.surface.stores.SurfaceMongoStore",
      "settingsFile": "SurfaceStorage.json"
    }
  ],
  "components":[
    {
      "name":"SolrIndexer",
      "jar":"DahuVector.jar",
      "class":"com.dahu.vector.indexers.solr.SolrCloudIndexerProcessor"
    },{
      "name":"DebugProcessor",
      "jar":"DahuVector.jar",
      "class":"com.dahu.vector.processors.DebugProcessor"
    },{
      "name":"SplitterProcessor",
      "jar":"DahuVector.jar",
      "class":"com.dahu.vector.processors.HierarchicalFieldSplitter"
    },
    {
      "name": "MimeTypePrettifier",
      "class": "com.dahu.vector.core.processors.MimeTypePrettifier"
    },
    {
      "name":"AODocsFetchInjector",
      "class":"com.dahu.aodocs.processors.DahuAODFetchInjector",
      "jar":"DahuEdge.jar"
    },
    {
      "name": "adminAuthenticator",
      "class": "com.dahu.plugins.core.auth.SimpleAuthenticator",
      "settingsFile": "AdminAuthenticator.json"
    },
    {
      "name": "googleAuthenticator",
      "class": "com.dahu.plugins.core.auth.Google.GoogleAuthenticator",
      "settingsFile": "GoogleAuthenticator.json"
    },
    {
      "name": "adminAuthorizer",
      "class": "com.dahu.plugins.core.auth.SimpleAuthorizer",
      "settingsFile": "AdminAuthorizor.json"
    }
  ],
  "queues":[
    {
      "name":"vector_queue",
      "persistent":"false"
    },
    {
      "name":"seed_queue",
      "persistent":"false"
    }
  ],
  "events": [
    {
      "name":"solrCommit",
      "jar":"DahuVector.jar",
      "class":"com.dahu.vector.indexers.solr.CommitToSolr",
      "settingsFile":"solrCommit.json"
    },{
      "name": "AODocsRefresh",
      "jar":"DahuEdge.jar",
      "class": "com.dahu.plugins.edge.aodocs.QueueSeederEvent",
      "settingsFile": "AODocsLibraries.json"
    }
  ]
}
