{
  "DEFSettings":{
    "inputQueues":["vector_queue"],
    "serviceGroup":"AODocs"
  },
  "pluginSettings": {
    "autoStart":"true",
    "components":{
      "AODocsFetchInjector":{
        "priority":"1",
        "pretika":"true",
      },
      "SplitterProcessor":{
        "priority":"1",
        "hier_metanames":"folderpath"
      },
      "MimeTypePrettifier": {
        "priority": "2"
      },
      "SolrIndexer": {
        "priority": "99",
        "solr_index":"aodocs_index",
        "cloud_solr_hostname":{"type": "env","values": {"env_name": "SOLR_HOSTNAME","default": "localhost"}},
        "solr_zkport":"8983",
        "logLevel":"INFO",
        "solrSchemaMap":"AODocsSolrSchema.json"
      }
    }
  }
}
